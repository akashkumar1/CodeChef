#include <bits/stdc++.h>
using namespace std;
long long int arr[100005];
int main(){
    long long int t;
    scanf("%lld",&t);
    while(t--){
        long long int i,n;
        scanf("%lld",&n);
        for(i=1;i<=n;i++)
           scanf("%lld",&arr[i]);
        bool flag=false;
        for(i=1;i<=n-2;i++)
            if(arr[i]==arr[i+1] && arr[i]==arr[i+2]){
                printf("Yes\n");
                flag=true;
                break;
            }
        if(flag==false) 
           printf("No\n");
    }
    return 0;
}